
{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "admin.conversations.rename",
  "description": "This Admin API method renames a public or private channel.",
  "type": "object",
  "properties": {
    "token": {
      "description": "Authentication token bearing required scopes.",
      "type": "string",
      "example": "xxxx-xxxxxxxxx-xxxx"
    },
    "channel_id": {
      "description": "The channel to rename.",
      "type": "string",
      "example": "C12345"
    },
    "name": {
      "description": "",
      "type": "string"
    }
  },
  "required": [
    "token",
    "channel_id",
    "name"
  ],
  "additionalProperties": false
}
